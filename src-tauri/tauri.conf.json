{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "clickthrough-w-draggable",
  "version": "0.1.0",
  "identifier": "com.clickthrough-w-draggable.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "clickthrough-w-draggable",
        "transparent": true,
        "decorations": false,
        "maximized": true,
        "alwaysOnTop": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}
